#MPSVac FBIS interface
#From MPSVac to FBIS. EPICS just reads.
#BPs and RDYs signals
record(bi, "MPSoS-MPSVac:Ctrl-PLC-001:MPSVac-BP1En")
{
    field(DTYP, "OPCUA")
    field(INP,  "@$(SUBS) ns=3;s=\"dbFBIS_Interface\".\"MPSVac_FBIS_FIO\".\"MPSBP1_Enbld\"")
    field(SCAN, "I/O Intr")
    field(ONAM, "BP1 Ok")
    field(ZNAM, "BP1 Not Ok")
    field(DESC, "MPSVac to FBIS Hardwired BP1")
}
